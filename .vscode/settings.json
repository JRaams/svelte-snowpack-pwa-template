{
  "i18n-ally.localesPaths": "src/locales",
  "i18n-ally.sourceLanguage": "en",
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "eslint.validate": [
    "javascript",
    "javascriptreact",
    "html"
  ],
  "eslint.packageManager": "yarn",
  "css.validate": false
}
